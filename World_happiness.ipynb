   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Plot#3 Mean happiness score per regional indicator (Bar chart - Nathan)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_regional = df.groupby('Regional indicator').mean()\n",
    "df_regional.reset_index(level=0, inplace=True)\n",
    "x_ri = df_regional[\"Regional indicator\"]\n",
    "y_ls_mean = df_regional[\"Ladder score\"]\n",
    "df_regional.head(10)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "plt.figure(figsize=(20,10))\n",
    "plt.bar(x_ri, y_ls_mean, width = 0.35)\n",
    "plt.ylim(bottom=4)\n",
    "plt.xticks(rotation=45, size = 12)\n",
    "plt.rc('axes', axisbelow=True)\n",
    "\n",
    "plt.ylabel(\"Mean happiness ladder score\", size=14)\n",
    "plt.title(\"Mean happiness ladder score per regional indicator\", size=16)\n",
    "plt.grid(True,axis = 'y')\n",
    "plt.show()"
   ]
